    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/node": "12.7.5",
    "@types/request": "^2.48.3",
    "request": "^2.88.0",
    "ts-node-dev": "^1.0.0-pre.42",
